          {
            "name": "take",
            "required": false,
            "in": "query",
            "description": "每页数量",
            "schema": { "default": 10, "type": "number" }
          },
          {
            "name": "skip",
            "required": false,
            "in": "query",
            "description": "跳过数量",
            "schema": { "default": 0, "type": "number" }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "object" } }
            }
          }
        },
        "tags": ["浏览历史记录"],
        "security": [{ "bearer": [] }]
      },
      "post": {
        "operationId": "BrowserTrackController_create",
        "summary": "创建历史记录",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostBrowserTrackBodyDTO"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "object" } }
            }
          }
        },
        "tags": ["浏览历史记录"],
        "security": [{ "bearer": [] }]
      },
      "delete": {
        "operationId": "BrowserTrackController_delete",
        "summary": "删除历史记录",
        "parameters": [
          {
            "name": "browserTrackID",
            "required": true,
            "in": "query",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "number" } }
            }
          }
        },
        "tags": ["浏览历史记录"],
        "security": [{ "bearer": [] }]
      }
    },
    "/broswer/track/all": {
