    "/broswer/mark/all": {
      "post": {
        "operationId": "BrowserMarkController_create",
        "summary": "更新所有书签",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PostBrowserMarkBodyDTO"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "object" } }
            }
          }
        },
        "tags": ["浏览器书签"],
        "security": [{ "bearer": [] }]
      }
    },
    "/subscribe/apple/user": {
      "get": {
        "operationId": "AppleController_getUserStatus",
        "summary": "获取苹果订阅状态",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "object" } }
            }
          }
        },
        "tags": ["苹果订阅"],
        "security": [{ "bearer": [] }]
      }
    },
