          {
            "name": "take",
            "required": true,
            "in": "query",
            "description": "每页数量",
            "schema": { "type": "number" }
          },
          {
            "name": "skip",
            "required": true,
            "in": "query",
            "description": "跳过数量",
            "schema": { "type": "number" }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "type": "object" } }
