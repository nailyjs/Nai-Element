    "/product": {
      "get": {
        "operationId": "ProductController_listProduct",
        "summary": "获取商品列表",
        "parameters": [
          {
            "name": "orderTime",
            "required": false,
            "in": "query",
            "description": "时间排序",
            "schema": {
              "default": "latest",
              "enum": ["latest", "oldest"],
              "type": "string"
            }
          },
          {
            "name": "orderHot",
            "required": false,
            "in": "query",
            "description": "热度排序",
            "schema": {
              "default": "hottest",
              "enum": ["hottest", "coldest"],
              "type": "string"
            }
          },
          {
            "name": "orderPrice",
            "required": false,
            "in": "query",
            "description": "价格排序",
            "schema": {
              "default": "highest",
              "enum": ["highest", "lowest"],
              "type": "string"
            }
          },
          {
            "name": "orderSold",
            "required": false,
            "in": "query",
            "description": "销量排序",
            "schema": {
              "default": "highest",
              "enum": ["highest", "lowest"],
              "type": "string"
            }
          },
          {
            "name": "orderStock",
            "required": false,
            "in": "query",
            "description": "库存排序",
            "schema": {
              "default": "highest",
              "enum": ["highest", "lowest"],
              "type": "string"
            }
          },
          {
            "name": "filterTags",
            "required": false,
            "in": "query",
            "description": "过滤标签",
            "schema": { "type": "array", "items": { "type": "number" } }
          },
          {
            "name": "filterUser",
            "required": false,
            "in": "query",
            "description": "过滤用户",
            "schema": { "type": "array", "items": { "type": "number" } }
          },
          {
            "name": "take",
            "required": false,
            "in": "query",
            "description": "取到数量",
            "schema": { "default": 10, "type": "number" }
          },
          {
            "name": "skip",
            "required": false,
            "in": "query",
            "description": "跳过数量",
            "schema": { "default": 0, "type": "number" }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": { "type": "array", "items": { "type": "object" } }
