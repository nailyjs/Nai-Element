          "ip": { "type": "string", "description": "IP", "default": "0.0.0.0" },
          "saying": {
            "type": "string",
            "description": "个性签名",
            "default": "这个人很懒，什么都没有写哦"
          },
          "avatar": {
            "type": "string",
            "description": "头像地址",
            "default": "https://avatars.githubusercontent.com"
          }
        },
        "required": ["userID", "createdAt", "updatedAt", "username", "ip", "saying", "avatar"]
      },
