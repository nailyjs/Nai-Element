        "type": "object",
        "properties": {
          "userID": { "type": "number", "description": "用户ID", "default": 1 },
          "createdAt": {
            "format": "date-time",
            "type": "string",
            "description": "注册时间"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string",
            "description": "更新时间"
          },
